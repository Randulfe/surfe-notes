    "eslint-plugin-storybook": "^0.11.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.7",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9",
    "vitest-browser-react": "^0.1.1"
